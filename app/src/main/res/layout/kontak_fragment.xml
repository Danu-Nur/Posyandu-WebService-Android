<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/kontak"
    android:background="@drawable/registrasi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.kontak.KontakFragment">

    <TextView
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Kader"
        android:textSize="25dp"
        android:textColor="#328E36"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_marginBottom="50dp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginTop="100dp">

        <ListView
            android:id="@+id/listKontak"
            tools:listitem="#24BEED"
            android:background="#24BEED"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false" />
    </RelativeLayout>

</FrameLayout>